{
  "server": "hub.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "bstack:options": {
      "buildName": "cucumber-java-browserstack",
      "local": "true",
      "debug": "true"
    }
  },

  "environments": [{
    "browserName": "chrome"
  }],

  "localOptions": {
    "forceLocal": true
  }
}
